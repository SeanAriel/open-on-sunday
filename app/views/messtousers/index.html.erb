<p id="notice"><%= notice %></p>

<h1>Messtousers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messtousers.each do |messtouser| %>
      <tr>
        <td><%= messtouser.title %></td>
        <td><%= messtouser.content %></td>
        <td><%= messtouser.user %></td>
        <td><%= link_to 'Show', messtouser %></td>
        <td><%= link_to 'Edit', edit_messtouser_path(messtouser) %></td>
        <td><%= link_to 'Destroy', messtouser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Messtouser', new_messtouser_path %>
